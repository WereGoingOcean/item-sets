    "url": "https://github.com/WereGoingOcean/item-sets/issues"
  },
  "homepage": "https://github.com/WereGoingOcean/item-sets#readme",
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-rc.5",
    "electron": "^9.2.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "vue-loader": "^16.0.0-beta.5",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "vue": "^3.0.0-rc.5"
  }
