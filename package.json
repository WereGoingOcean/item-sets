{
  "name": "item-sets",
  "version": "0.1.0",
  "description": "League item set creator",
  "main": "./dist/main.js",
  "scripts": {
    "start": "npm run build-electron && npm run build-vue && electron .",
    "build": "npm run build-electron && npm run build-vue",
